package Modificadores;

public class Estado {

	private String nome;
	private String sigla;
	
	public void Estado(String nome, String sigla) {      
	}
	public String getNome() {
	    return nome;
	}
	public String getSigla() {
	    return sigla;
	}
	public void setNome(String nome) {
	    this.nome = nome;
	}
	public void setSigla(String sigla) {
	    this.sigla = sigla;
	    
	}	

}
